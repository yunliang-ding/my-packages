{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "declaration": true,
    "jsx": "react-jsx",
    "strict": false,
    "resolveJsonModule": true,
    "sourceMap": false,
    "skipLibCheck": true,
    "outDir": "./dist",
    "paths": {
      "react": [
        "../../node_modules/@types/react"
      ],
    }
  },
  "exclude": [
    "node_modules",
    "./dist"
  ]
}