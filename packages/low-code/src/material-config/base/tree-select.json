{
  "type": "TreeSelect",
  "label": "树形下拉选",
  "props": {
    "placeholder": "请选择",
    "allowClear": true,
    "disabled": false,
    "treeData": [
      {
        "title": "Node1",
        "value": "0-0",
        "children": [
          {
            "title": "Child Node1",
            "value": "0-0-1"
          },
          {
            "title": "Child Node2",
            "value": "0-0-2"
          }
        ]
      },
      {
        "title": "Node2",
        "value": "0-1"
      }
    ]
  },
  "propsConfig": [
    {
      "type": "Switch",
      "name": "disabled",
      "label": "是否禁用"
    },
    {
      "type": "Switch",
      "name": "allowClear",
      "label": "是否允许清除"
    },
    {
      "type": "Input",
      "name": "placeholder",
      "label": "提示文案"
    },
    {
      "type": "CodeEditor",
      "label": "设置数据源",
      "name": "treeData",
      "tooltip": "仅支持json",
      "props": {
        "mode": "json"
      }
    },
    {
      "type": "CodeEditor",
      "name": "onChange",
      "label": "设置onChange",
      "props": {
        "mode": "function",
        "useEncrypt": true
      }
    }
  ]
}
