{
  "compilerOptions": {
    "lib": ["es6", "dom"],
    "target": "es2016",
    "module": "esnext",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "declaration": true,
    "jsx": "react-jsx",
    "strict": false,
    "sourceMap": false,
    "skipLibCheck": true,
    "outDir": "./dist",
    "paths": {
      "@/*": ["./src/*"],
      "lyr": ["./src/.lyr"],
      "react": ["./node_modules/@types/react"]
    }
  },
  "exclude": ["node_modules"]
}
